{
  "agents": {
    "defaults": {
      "model": {
        "primary": "anthropic/claude-sonnet-4-20250514"
      },
      "workspace": "/data/workspace",
      "contextTokens": 80000,
      "contextPruning": { "mode": "cache-ttl", "ttl": "10m" },
      "compaction": {
        "mode": "default",
        "reserveTokens": 40000,
        "keepRecentTokens": 25000,
        "reserveTokensFloor": 40000
      },
      "bootstrapMaxChars": 35000,
      "heartbeat": { "every": "1h" },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      }
    }
  }
}
